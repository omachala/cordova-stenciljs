{
  "name": "com.omachala.life",
  "displayName": "HelloWorld",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve --config ./stencil.config.dev.json",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "author": "Ondrej Machala",
  "license": "MIT",
  "dependencies": {
    "@ionic/core": "4.0.0",
    "@stencil/core": "0.17.0",
    "@stencil/router": "~0.3.1",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-whitelist": "^1.3.3",
    "gsap": "2.0.2"
  },
  "devDependencies": {
    "@stencil/webpack": "0.0.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "workbox-build": "3.4.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "ios"
    ]
  }
}
